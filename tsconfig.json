{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "olive/*": [
        "lib/olive.mvc/typings/*"
      ],
      "echarts": [
        "lib/echarts/dist/echarts"
      ],
      "zrender": [
        "lib/zrender/dist"
      ],
      "app/*": [
        "src/*"
      ]
    },
    "types": [],
    "skipLibCheck": true,
    "typeRoots": [
      "lib/olive.mvc/typings/*",
      "lib/olive.mvc/typings-lib/*",
      "node_modules/@types/*",
      "lib/@types/*",
      "dist/types/hub/*",
      "lib/echarts/types/dist/*",
      "./node_modules/@types"
    ],
    "module": "amd",
    "target": "es6",
    "outDir": "compiled",
    "declaration": true,
    "declarationDir": "dist/types/hub",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false
  },
  "compileOnSave": true,
  "exclude": [
    "lib/olive.mvc/src",
    "lib/find-up",
    "lib/ansi-styles",
    "dist/types/hub/*",
    "lib/yargs",
    "bin",
    "obj",
    "tsconfig.json",
    "lib"
  ]
}